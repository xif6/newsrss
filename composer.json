{
    "name": "xif6/newsrss",
    "license": "MIT",
    "type": "project",
    "description": "NewsRSS.net the rss aggregator",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/xif6/DoctrineExtensions.git"
        },
        {
            "type": "package",
            "package": {
                "name": "google/angularjs",
                "version": "1.2.16",
                "dist": {
                    "url": "http://ajax.googleapis.com/ajax/libs/angularjs/1.2.16/angular.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "2.1.0",
                "dist": {
                    "url": "http://ajax.googleapis.com/ajax/libs/jquery/2.1.0/jquery.min.js",
                    "type": "file"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.6",
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.1",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.6",
        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",
        "incenteev/composer-parameter-handler": "~2.1",
        "gedmo/doctrine-extensions": "dev-wip-v2.4.0",
        "friendsofsymfony/user-bundle": "2.0.*@dev",
        "doctrine/mongodb-odm": "1.0.*@dev",
        "doctrine/mongodb-odm-bundle": "3.0.*@dev",
        "jms/serializer-bundle": "0.13.*@dev",
        "jquery/jquery": "*",
        "google/angularjs": "*",
        "oyejorge/less.php": "dev-master",
        "twbs/bootstrap": "3.2.*",
        "knplabs/knp-menu": "2.0.*@dev",
        "knplabs/knp-menu-bundle": "2.0.*@dev",
        "knplabs/knp-paginator-bundle": "2.4.*@dev",
        "mopa/bootstrap-bundle": "3.*@dev",
        "friendsofsymfony/elastica-bundle": "3.1.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*@dev",
        "phpunit/phpunit-mock-objects": "2.0.*@dev"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallMirrorTwitterBootstrap"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallMirrorTwitterBootstrap"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
